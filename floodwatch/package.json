{
  "name": "floodwatch",
  "version": "0.2.0",
  "private": true,
  "plugins": [
    "esformatter-jsx"
  ],
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chokidar-cli": "^1.2.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.46.0",
    "flow-interfaces-chrome": "^0.1.1",
    "node-sass": "^3.13.0",
    "prettier": "^1.3.1",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "d3": "3.5.17",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "loglevel": "^1.4.1",
    "moment": "^2.18.1",
    "react": "^15.3.2",
    "react-autocomplete": "^1.4.0",
    "react-bootstrap": "^0.30.6",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "scroll-to": "^0.0.2",
    "whatwg-fetch": "^1.0.0"
  },
  "scripts": {
    "start": "yarn sass-build && react-scripts start",
    "build": "yarn sass-build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "sass-watch": "chokidar 'src/sass/**/*.scss' --initial -c 'yarn run sass-build'",
    "sass-build": "node-sass src/sass/App.scss > src/css/app.css",
    "lint": "./node_modules/eslint/bin/eslint.js src",
    "pretty": "prettier --single-quote --trailing-comma es5 --jsx-bracket-same-line=true --parser flow --write \"src/**/*.js\"",
    "fix": "yarn pretty && yarn lint -- --fix"
  }
}
